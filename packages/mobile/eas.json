{
	"cli": {
		"appVersionSource": "remote",
		"version": ">= 16.3.2"
	},
	"build": {
		"development": {
			"channel": "develop",
			"developmentClient": true,
			"distribution": "internal"
		},
		"preview": {
			"channel": "preview",
			"distribution": "internal"
		},
		"production": {
			"channel": "production",
			"autoIncrement": true,
			"env": {
				"EXPO_PUBLIC_MODE": "production",
				"NODE_ENV": "production"
			}
		}
	},
	"submit": {
		"production": {
			"android": {
				"releaseStatus": "draft"
			}
		}
	}
}
